{
  "manifest_version": 3,
  "name": "EDM Download Interceptor",
  "version": "1.0",
  "permissions": ["downloads", "downloads.open", "downloads.shelf", "storage"],
  "host_permissions": ["http://localhost:5500/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
